package com.tolgakmbl.hrms.entities.concretes;

import javax.persistence.*;
import javax.validation.constraints.*;

import lombok.*;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Builder
@ToString
@EqualsAndHashCode
@Entity
@Table(name = "LANGUAGES")
public class Language {
	
	@Column(name = "ID")
	@Id
	@Size(min = 2, max = 2)
	@NotBlank
	private String id;

	@NotBlank
	@Column(name = "NAME")
	private String name;

}
