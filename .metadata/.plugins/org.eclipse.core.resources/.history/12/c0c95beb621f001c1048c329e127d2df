package com.tolgakmbl.hrms.entities.concretes;

import javax.persistence.*;

import lombok.*;

@Entity
@Data
@NoArgsConstructor
@AllArgsConstructor
@Table(name="employers")
public class Employer extends User{
	
	private int user_id;
	private String company_name;
	private String website;
	private String phone_number;	
}
